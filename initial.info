name = Initial
description = A not-quite single-page Drupal 7 theme.
screenshot = assets/images/screenshot.png
core = 7.x
php = 5.2

;--FEATURES--
features[] = logo

;--SETTINGS--
settings[toggle_logo] = 0

;--REGIONS--

;--required regions--
regions[page_top] = Page Top
regions[content] = Content
